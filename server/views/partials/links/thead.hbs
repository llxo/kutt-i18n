<thead>
  <tr class="controls links-controls">
    <th class="search">
      <input class="table-input search" id="search" name="search" type="text" placeholder="{{i18n 'common.search'}}..." hx-on:keyup="resetTableNav()" />
      <input id="total" name="total" type="hidden" value="{{total}}" />
      <input id="limit" name="limit" type="hidden" value="10" />
      <input id="skip" name="skip" type="hidden" value="0" />
    </th>
    {{> links/nav}}
  </tr>  <tr>
    <th class="original-url">{{i18n 'table.originalUrl'}}</th>
    <th class="created-at">{{i18n 'table.createdAt'}}</th>
    <th class="short-link">{{i18n 'table.shortUrl'}}</th>
    <th class="views">{{i18n 'table.clicks'}}</th>
    <th class="actions">{{i18n 'table.actions'}}</th>
  </tr>
</thead>